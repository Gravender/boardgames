{
  "name": "@board-games/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "default": "./src/client.ts"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "default": "./src/schema/index.ts"
    },
    "./zodSchema": {
      "types": "./dist/zodSchema.d.ts",
      "default": "./src/zodSchema/index.ts"
    },
    "./constants": {
      "types": "./dist/constants.d.ts",
      "default": "./src/constants/index.ts"
    },
    "./views": {
      "types": "./dist/views.d.ts",
      "default": "./src/views/index.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "push": "pnpm with-env drizzle-kit push",
    "migrate": "pnpm with-env drizzle-kit migrate",
    "generate": "pnpm with-env drizzle-kit generate",
    "seed": "pnpm with-env tsx src/seeding/seed.ts",
    "studio": "pnpm with-env drizzle-kit studio",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@vercel/postgres": "^0.10.0",
    "drizzle-orm": "1.0.0-beta.1-5e64efc",
    "drizzle-zod": "^0.8.3",
    "postgres": "^3.4.5",
    "tsx": "^4.19.2",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@board-games/eslint-config": "workspace:*",
    "@board-games/prettier-config": "workspace:*",
    "@board-games/tsconfig": "workspace:*",
    "@faker-js/faker": "^9.6.0",
    "@types/d3": "^7.4.3",
    "d3": "^7.9.0",
    "date-fns": "^4.1.0",
    "dotenv-cli": "^10.0.0",
    "drizzle-kit": "1.0.0-beta.1-5e64efc",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@board-games/prettier-config"
}
